{
    "name": "julienbohy/typescript-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Acme bundle description",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.4|^7.4|^8.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JulienBohy\\TypescriptBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JulienBohy\\TypescriptBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.65"
    }
}
